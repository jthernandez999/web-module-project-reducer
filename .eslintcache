        <span id=\"operation\"><b>Operation:</b> {state.operation}</span>\n              <span id=\"memory\"><b>Memory:</b> {state.memory}</span>\n            </div>\n            \n            <div className=\"row\">\n              <CalcButton onClick={addToMemoryHandler} value={\"M+\"}/>\n              <CalcButton onClick={() => {applyNumberHandler(state.memory)}} value={\"MR\"}/>\n              <CalcButton onClick={clearMemoryHandler} value={\"MC\"}/>\n            </div>\n\n            <div className=\"row\">\n              <CalcButton onClick={() => {handleNumber(1)}} value={1}/>\n              <CalcButton onClick={() => {handleNumber(2)}} value={2}/>\n              <CalcButton onClick={() => {handleNumber(3)}} value={3}/>\n            </div>\n\n            <div className=\"row\">\n              <CalcButton onClick={() => {handleNumber(4)}} value={4}/>\n              <CalcButton onClick={() => {handleNumber(5)}} value={5}/>\n              <CalcButton onClick={() => {handleNumber(6)}} value={6}/>\n            </div>\n\n            <div className=\"row\">\n              <CalcButton onClick={() => {handleNumber(7)}} value={7}/>\n              <CalcButton onClick={() => {handleNumber(8)}} value={8}/>\n              <CalcButton onClick={() => {handleNumber(9)}} value={9}/>\n            </div>\n\n            <div className=\"row\">\n              <CalcButton onClick={() => {operationHandler('+')}} value={\"+\"}/>\n              <CalcButton onClick={() => {operationHandler('*')}} value={\"*\"}/>\n              <CalcButton onClick={() => {operationHandler('-')}}value={\"-\"}/>\n            </div>\n\n            <div className=\"row ce_button\">\n              <CalcButton onClick={clearHandler} value={\"CE\"}/>\n            </div>\n\n          </form>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/joehernandez/WEBPT31/UNIT 3/3.2/PROJECTS/web-module-project-reducer/src/components/CalcButton.js",[],"/Users/joehernandez/WEBPT31/UNIT 3/3.2/PROJECTS/web-module-project-reducer/src/components/TotalDisplay.js",[],"/Users/joehernandez/WEBPT31/UNIT 3/3.2/PROJECTS/web-module-project-reducer/src/reducers/index.js",["38"],"import { ADD_ONE, APPLY_NUMBER, CHANGE_OPERATION, CLEAR_DISPLAY, ADD_TO_MEMORY, CLEAR_MEMORY } from './../actions';\n\nexport const initialState = {\n    total: 0,\n    operation: \"+\",\n    memory: 0\n}\n\nconst calculateResult = (num1, num2, operation) => {\n    switch(operation) {\n        case(\"+\"):\n            return num1 + num2;\n        case(\"*\"):\n            return num1 * num2;\n        case(\"-\"):\n            return num1 - num2;\n    }\n}\n\nexport const reducer = (state, action) => {\n    switch(action.type) {\n        case(ADD_ONE):\n            return({\n                ...state,\n                total: state.total + 1\n            });\n\n        case(APPLY_NUMBER):\n            return ({ \n                ...state, \n                total: calculateResult(state.total, action.payload, state.operation)\n            });\n        \n        case(CHANGE_OPERATION):\n            return ({\n                ...state,\n                operation: action.payload\n            });\n        case(CLEAR_DISPLAY):\n            return ({\n                ...state, \n                total: state.total = 0\n            });\n        case(ADD_TO_MEMORY):\n        return({\n            ...state, \n            memory: state.total\n        })\n        case(CLEAR_MEMORY):\n        return({\n            ...state, \n            memory: 0\n        })\n        default:\n            return state;\n    }\n}\n\nexport default reducer;\n","/Users/joehernandez/WEBPT31/UNIT 3/3.2/PROJECTS/web-module-project-reducer/src/actions/index.js",[],{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":9,"column":10,"nodeType":"45","messageId":"46","endLine":9,"endColumn":16},{"ruleId":"47","severity":1,"message":"48","line":46,"column":9,"nodeType":"